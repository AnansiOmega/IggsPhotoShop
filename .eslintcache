h, Route } from 'react-router-dom'\nimport { Home } from './pages/home'\nimport { Login } from './pages/login'\nimport { Cart } from './pages/cart'\nimport { About } from './pages/about'\nimport { NavBar } from './components/navbar'\nimport image from './images/blackWoodBackground.jpg'\nimport { currentUser } from './Actions/auth'\nimport { useDispatch } from 'react-redux'\nimport { fetchPhotosSuccess } from './Actions/photos'\n\nexport const App = () => {\n  const dispatch = useDispatch()\n  useEffect(() => {\n    document.body.style.backgroundImage = `url(${image})`\n  },[])\n\n  useEffect(() => {\n    const token = localStorage.getItem('myToken')\n    const reqObj = {\n        method: 'GET',\n        headers: {\n            'Authorization': `Bearer ${token}`\n        }\n    }\n    fetch('https://desolate-plateau-74310.herokuapp.com/current_user', reqObj)\n    .then(resp => resp.json())\n    .then(user => dispatch(currentUser(user)))\n  },[])\n\n  useEffect(() => {\n  fetch('https://desolate-plateau-74310.herokuapp.com/photos')\n  .then(resp => resp.json())\n  .then(photos => dispatch(fetchPhotosSuccess(photos)))\n  },[])\n\n\n\n  return (\n    <div className=\"App\">\n      <NavBar />\n       <Switch>\n          <Route exact path='/' component={Home} />\n          <Route path='/login' component={Login} />\n          <Route path='/cart' component={Cart} />\n          <Route path='/about' component={About} />\n        </Switch>\n    </div>\n  );\n}",["62","63"],"/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/Actions/auth.js",[],["64","65"],"/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/Reducers/cart.js",[],"/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/pages/home.js",["66"],"import React, { useEffect, useState } from 'react'\nimport { useSelector } from 'react-redux'\nimport { PhotoCard } from '../components/PhotoCard'\nimport { Lightbox } from \"react-modal-image\";\n\nexport const Home = () => {\nconst [ open, setOpen ] = useState(false)\nconst [loader, setLoader] = useState(false)\nconst photos = useSelector(state => state.photos)\nconst photo = useSelector(state => state.photo)\nuseEffect(()=> {\n    photos.length < 1 ? setLoader(true) : setLoader(false)\n})\n\n\nconst renderPhotos = () => {\n    return photos.map(photo => <PhotoCard setOpen={setOpen} photo={photo} key={photo.id}/>)\n}\n\nconst { title, image } = photo\nlet url = `https://desolate-plateau-74310.herokuapp.com${image}`\n\n    return(\n        <>\n            { open ? \n             <Lightbox\n             large={url}\n             alt={title}\n             onClose={() => setOpen(false)}\n             hideDownload={true}\n            /> \n            :\n            null\n            }\n            {loader ?  <><h1 style={{color: 'white'}}>Loading Images, this might take a while</h1><p style={{color: 'white'}}>my apologies, I'm using free services</p></> : <div className=\"card-container\">{renderPhotos()}</div>}\n        </>\n    )\n\n}","/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/components/navbar.js",["67"],"/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/pages/cart.js",["68","69","70"],"import React, { useState } from 'react'\nimport { connect, useDispatch, useSelector } from 'react-redux'\n// import { PayPalButton } from \"react-paypal-button-v2\";\nimport { useHistory } from 'react-router-dom'\nimport { clearCart } from '../Actions/photos'\nimport { removeFromCart } from '../Actions/photos'\n\n\nexport const Cart = () => {\n    const [ paypalButton, setPaypalButton ] = useState(false)\n    const cart = useSelector(state => state.cart, prev => prev.length === cart)\n\n\n    const dispatch = useDispatch()\n\n\n    const history = useHistory()\n    const total = cart.map(photo => parseInt(photo.price)).reduce((a, b) => a + b, 0)\n\n    const renderCart = () => {\n        return cart.map((photo, i) => {\n        return <div className='cart-item'>{photo.title}: ${photo.price}.00\n        <button onClick={() => dispatch(removeFromCart(i))} style={{float: 'right'}} className=\"circular mini ui icon button black\">\n        <i className=\"x icon\"></i>\n        </button></div>\n        })\n    }\n\n    const handleCheckout = () => {\n        dispatch(clearCart())\n        history.push('/')\n    }\n\n    return(\n        <>\n            { !paypalButton ? \n            <>\n            <div style={{ fontSize: '20px'}} className='login-form'>Please Read the About page before you checkout</div>\n            <div className='cart-cont'>\n                <div className='your-cart'>Your cart</div>\n                    { cart.length !== 0 ? <div className='cart'>{renderCart()}</div> : null }\n                <div className='total'>Total: ${total}.00</div>\n                <a href='https://www.buymeacoffee.com/AnansiOmega' target='_blank'><button onClick={handleCheckout} style={{ marginLeft: '5px', width: '497px', color: 'black' }} className='massive ui button'>Make a Donation</button></a>\n            </div>\n            </>\n            :\n            <>\n                {/* <div className='total-paypal'>Total: ${total}.00</div> */}\n\n                {/* <PayPalButton\n                amount={total}\n                shippingPreference= \"NO_SHIPPING\"\n                options={{\n                    clientId: \"ASenCKzw7AEnV5LN5wv2E8TLaZdIpCKZIZYePB0et7L1JA-zippvq-5HFKgUi-BKXCaXtPd24I34I_SM\"\n                }}\n                onSuccess={(details, data) => { \n                alert(\"Thanks for donating \" + details.payer.name.given_name)\n                dispatch(clearCart())\n                history.push('/')\n                }}\n                /> */}\n            </>\n            }\n        </>\n    )\n}\n","/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/index.js",[],["71","72"],"/Users/anansi/Development/code/projects/myPhotoShop/iggsphotoshop/src/Actions/photos.js",[],"/Users/anans